#include <iostream>
#include <string>
using namespace std;
//本人太笨，只能用这种极其简单的方法解决这道题
float avescroe(int x)
{
	int as;
	int array[3][5] = { {85,78,99,96,88},{76,89,75,97,75},{64,92,90,73,56} };
	as = float(array[x - 1][0] + array[x - 1][1] + array[x - 1][2] + array[x - 1][3] + array[x - 1][4]) / 5;
	return as;
}
